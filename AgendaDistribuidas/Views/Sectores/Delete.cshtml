@model AgendaDistribuidas.Models.Sector

@{
    ViewData["Title"] = "Elimiar";
}

<h2>Eliminar</h2>

<h3>¿Estás seguro de querer eliminar esto?</h3>
<div>
    <h4>Sector</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Sector
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Descripcion)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="SectorId" />
        <input type="submit" value="Eliminar" class="btn btn-default" /> |
        <a asp-action="Index">Volver atrás</a>
    </form>
</div>
